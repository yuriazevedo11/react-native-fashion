    "@types/react-native": "~0.62.13",
    "@types/styled-components": "^5.1.1",
    "@types/yup": "^0.29.3",
    "@yuriazevedo/cz-emoji": "^1.1.1",
    "@yuriazevedo/eslint-config-react-native": "^1.1.1",
    "commitizen": "^4.1.2",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "~3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@yuriazevedo/cz-emoji"
    }
